<?php

foreach ($block->getProductList() as $product) {
    if ($product) {
        $result = "";
        $productName = $product->getName();
        $productPrice = $product->getPrice();
        $result = $productName . ' => ' . $productPrice;
        echo $result . '<br />';
    } else {
        echo "No product found!";
    }
}
